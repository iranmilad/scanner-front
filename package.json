{
  "name": "tradersarena",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@mantine/core": "^4.2.12",
    "@mantine/hooks": "^4.2.12",
    "@mantine/modals": "^4.2.12",
    "@mantine/notifications": "^4.2.12",
    "@reduxjs/toolkit": "^1.7.1",
    "@tanstack/react-query": "^4.16.1",
    "@tanstack/react-query-devtools": "^4.16.1",
    "anychart": "^8.11.0",
    "anychart-react": "^1.4.1",
    "apexcharts": "^3.33.0",
    "autoprefixer": "^10.4.2",
    "axios": "^0.25.0",
    "axios-mock-adapter": "^1.21.2",
    "firebase": "^9.9.3",
    "formik": "^2.2.9",
    "js-cookie": "^3.0.1",
    "lightweight-charts": "^3.8.0",
    "lodash": "^4.17.21",
    "match-sorter": "^6.3.1",
    "path-to-regexp": "^6.2.1",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-nested": "^5.0.6",
    "react": "^17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-cookie": "^4.1.1",
    "react-data-table-component": "^7.4.7",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.0",
    "redux": "^4.1.2",
    "styled-components": "^5.3.5",
    "stylis": "^4.0.13",
    "stylis-plugin-rtl": "^2.1.1",
    "tailwindcss": "^3.0.18",
    "url-parse": "^1.5.10",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "build-css": "postcss src/index.css -o src/assets/css/tailwind.css",
    "watch-css": "postcss src/index.css -o src/assets/css/tailwind.css -w",
    "start": "concurrently -k  \"npm run watch-css\" \"npm run start-js\" ",
    "start-js": "react-scripts start",
    "build-js": "set \"GENERATE_SOURCEMAP=false\" && react-scripts build",
    "build": "concurrently \"npm run build-css\" \"npm run build-js\" ",
    "config": "node setconfig.js",
    "test": "cypress open",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/node-logger": "^6.5.13",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@types/react-redux": "^7.1.22",
    "babel-plugin-named-exports-order": "^0.0.2",
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^11.1.0",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.75.0",
    "yargs": "^17.6.0"
  }
}
